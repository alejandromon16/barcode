{
  "expo": {
    "name": "barcode",
    "slug": "barcode",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "barcode",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "owner": "alm1",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alm1.barcode"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.alm1.barcode"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "GT Walsheim Pro-regular": [
            "/src/assets/fonts/GTWalsheimPro-Regular.ttf"
          ]
        }
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b929760b-8add-4114-8398-01ed7b5c9882"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b929760b-8add-4114-8398-01ed7b5c9882"
    }
  }
}
