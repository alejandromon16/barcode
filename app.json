{
  "expo": {
    "name": "barcode",
    "slug": "barcode",
    "version": "1.0.1",
    "orientation": "portrait",
    "scheme": "barcode",
    "icon": "./src/assets/images/icon.png",
    "userInterfaceStyle": "light",
    "owner": "alm1",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#0D823D"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.alm1.barcode",
      "infoPlist": {
        "NSPhotoLibraryUsageDescription": "This app uses the camera to scan barcodes on tickets verifications"
      },
      "buildNumber": "5"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/adaptive-icon.png",
        "backgroundColor": "#0D823D"
      },
      "package": "com.alm1.barcode"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "GT Walsheim Pro-regular": [
            "/src/assets/fonts/GTWalsheimPro-Regular.ttf"
          ],
          "GT Walsheim Pro-bold": [
            "/src/assets/fonts/GTWalsheimPro-Bold.ttf"
          ]
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      [
        "expo-barcode-scanner"
      ]
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "b929760b-8add-4114-8398-01ed7b5c9882"
      }
    },
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/b929760b-8add-4114-8398-01ed7b5c9882"
    }
  }
}
